class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        size = len(nums)
        if size > 2 :
        	for i in range (0,size - 1):
        	    for j in range (i + 1,size):
        	        if nums[i] + nums[j] == target:
        				return [i,j]
        else:
            return
